{
  "$schema": "https://typedoc.org/schema.json",
  "extends": ["./typedoc.base.json"],
  "name": "US Legal Tools SDK Documentation",
  "entryPoints": [
    "packages/*/src/api/generated/endpoints.ts",
    "packages/*/src/api/generated/model/index.ts"
  ],
  "entryPointStrategy": "expand",
  "exclude": [
    "**/test-sdk.ts",
    "**/scripts/**",
    "**/mcp/**",
    "**/client.ts",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/tests/**",
    "**/e2e/**",
    "**/integration/**"
  ],
  "out": "docs"
}
