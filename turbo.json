{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**"]
    },
    "build:clean": {
      "cache": false
    },
    "build:js": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:types": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "generate": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test:integration": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test:e2e": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test:all": {
      "dependsOn": ["build"],
      "cache": false
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "check": {
      "outputs": []
    },
    "check:ci": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "mcp:server": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "//#format-and-lint": {},
    "//#format-and-lint:fix": {
      "cache": false
    }
  }
}
